<?xml version="1.0" encoding="utf-8"?>
<resources>


    <!--认证中心进度条-->
    <declare-styleable name="AuthenticateCenterProgressBar">
        <attr name="progressIndicator" format="reference" />
        <attr name="textSize" format="dimension" />
        <attr name="textColors" format="reference|color" />
        <attr name="textStyle">
            <flag name="normal" value="0" />
            <flag name="bold" value="1" />
            <flag name="italic" value="2" />
        </attr>
        <attr name="textAlign">
            <flag name="left" value="0" />
            <flag name="center" value="1" />
            <flag name="right" value="2" />
        </attr>
        <attr name="offset" format="dimension" />
    </declare-styleable>

</resources>